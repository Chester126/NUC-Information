@import "../../../utils/weui.wxss";

.add_icon {
  position: fixed;
  width: 42px;
  height: 42px;
  bottom: 30px;
  right: 20px;
  z-index: 999;
}

.url {
  margin-top: 2%;
  font: 50%;
  color: #413c3c6e;
}

.table {
  display: inline-flex;
  flex-direction: column;
  border: 1rpx solid rgba(218, 217, 217, 1);
  border-bottom: 0;
}

.scrollClass {
  display: flex;
  width: 100%;
  white-space: nowrap;
  height: 100%;
  background-color: white;
}

.table_header {
  display: inline-flex;
}

.th1 {
  display: flex;
  flex-direction: column;
  width: 550rpx;
  height: 70rpx;
  background: #f8f8f8;
  border-right: 1rpx solid rgba(218, 217, 217, 1);
  border-bottom: 1rpx solid rgba(218, 217, 217, 1);
  justify-content: center;
  align-items: center;
  overflow-x: auto;
}

.th2 {
  display: flex;
  flex-direction: column;
  width: 100rpx;
  height: 70rpx;
  background: #f8f8f8;
  border-right: 1rpx solid rgba(218, 217, 217, 1);
  border-bottom: 1rpx solid rgba(218, 217, 217, 1);
  justify-content: center;
  align-items: center;
  overflow-x: auto;
}

.cell_label {
  font-size: 26rpx;
  color: rgba(74, 74, 74, 1);
}

.cell_date_label {
  font-size: 20rpx;
  color: rgba(74, 74, 74, 1);
}

.table_main {
  display: inline-flex;
  flex-direction: row;
}

.right-item {
  display: flex;
  flex-direction: row;
}

.td1 {
  display: flex;
  flex-direction: column;
  width: 550rpx;
  /* height: 90rpx; */
  background: white;
  justify-content: center;
  align-items: center;
  border: 1rpx solid rgba(218, 217, 217, 1);
  border-top: 0;
  border-left: 0;
}

.td2 {
  display: flex;
  flex-direction: column;
  width: 100rpx;
  /* height: 90rpx; */
  background: white;
  justify-content: center;
  align-items: center;
  border: 1rpx solid rgba(218, 217, 217, 1);
  border-top: 0;
  border-left: 0;
}

.table_Text_class {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 60rpx;
  font-size: 30rpx;
  color: #3c3f3f;
  width: auto;
  word-break: normal;
  /*border-bottom: 1rpx solid rgba(218, 217, 217, 1);*/
}
