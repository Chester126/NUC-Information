.text {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 6;
}

.sw {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5); /*蒙层的透明度可以自定义*/
  z-index: 9999999999;
}

.add_icon {
  position: fixed;
  width: 42px;
  height: 42px;
  bottom: 30px;
  right: 20px;
  z-index: 99999999;
}

